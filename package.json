{
  "name": "toy-train-react",
  "version": "0.1.0",
  "description": "A simple React app base built with Parcel.  Intended for use with toy projects.",
  "main": "index.js",
  "scripts": {
    "build": "parcel build index.html",
    "lint": "prettier --check src/** && eslint --fix src/**",
    "lint-check": "eslint --print-config src/**/*.js | eslint-config-prettier-check",
    "start": "parcel index.html",
    "test": "jest --watch"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/setupTests.js"
    ]
  },
  "keywords": [
    "React",
    "Parcel",
    "Jest",
    "Enzyme"
  ],
  "author": "haworku",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.25.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "1.17.0"
  }
}
